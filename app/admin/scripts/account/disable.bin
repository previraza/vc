randomize = (length = 12, prefix = "MCI", separator = "-") => { let res = pre_res = ""; for (let i = 0; i < length; i++) pre_res = parseInt(35 * Math.random()).toString(35), res += (i % 4 ? "" : separator) + (2 * Math.random() > 1 ? pre_res.toUpperCase() : pre_res); return prefix + separator + res }, mci_generator_enc = (id = 0, name = "Undefined") => { $("#contentKEY").val(randomize(16, "enc_", "")), $("#contentSSID").val(randomize()), $("#modalTitle").text(name), $("#contentID").val(id), $("#submitFormulary")[0].dataset.id = id, $("#modelId").modal() }, $("#submitFormulary").on("submit", e => { e.preventDefault(), updateProfile(e) }), updateProfile = e => {
